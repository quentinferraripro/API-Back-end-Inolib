{
  "name": "@inolib/api",
  "version": "0.0.0",
  "private": true,
  "description": "INOLIBâ€™s GraphQL API",
  "type": "module",
  "scripts": {
    "build": "pnpm run build:types && pnpm run build:api",
    "build:api": "vite build",
    "build:types": "pnpm run build:types.gql && pnpm run build:types.prisma",
    "build:types.gql": "vite-node ./src/scripts/build/types.gql/pre.ts && graphql-codegen",
    "build:types.prisma": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "vite-node ./prisma/seed.ts",
    "db:studio": "prisma studio --browser none",
    "format": "pnpm run format:prettier && pnpm run format:jsdoc && pnpm run format:packagejson && pnpm run format:prisma && pnpm run format:sort-imports",
    "format:jsdoc": "prettier --config ./.config/prettier/jsdoc/.prettierrc.cjs --write ./**/*.cjs ./**/*.ts",
    "format:packagejson": "prettier --config ./.config/prettier/packagejson/.prettierrc.cjs --write ./package.json",
    "format:prettier": "prettier --write .",
    "format:prisma": "prettier --config ./.config/prettier/prisma/.prettierrc.cjs --write ./**/*.prisma",
    "format:sort-imports": "prettier --config ./.config/prettier/sort-imports/.prettierrc.cjs --write ./**/*.ts",
    "lint": "pnpm run lint:eslint && pnpm run lint:tsc",
    "lint:eslint": "eslint --ext .cjs,.ts .",
    "lint:tsc": "tsc",
    "serve:dev": "pnpm run build && dotenv -- vercel dev",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@envelop/graphql-modules": "^4.0.6",
    "@prisma/client": "^4.16.2",
    "graphql": "^16.7.1",
    "graphql-modules": "^2.2.0",
    "graphql-scalars": "^1.22.2",
    "graphql-yoga": "^3.9.1",
    "nodemailer": "^6.9.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/graphql-modules-preset": "^3.1.3",
    "@graphql-codegen/typescript": "^3.0.4",
    "@graphql-codegen/typescript-resolvers": "^3.2.1",
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@rollup/plugin-graphql": "^2.0.3",
    "@types/node": "^18.17.3",
    "@types/nodemailer": "^6.4.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/node": "^2.15.8",
    "@vitest/coverage-c8": "^0.30.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "glob": "^10.3.3",
    "graphql-config": "^4.5.0",
    "prettier": "^2.8.8",
    "prettier-plugin-jsdoc": "^0.4.2",
    "prettier-plugin-packagejson": "^2.4.5",
    "prettier-plugin-prisma": "^4.17.0",
    "prisma": "^4.16.2",
    "typescript": "^5.1.6",
    "vercel": "^30.2.3",
    "vite": "^4.4.9",
    "vite-node": "^0.30.1",
    "vite-plugin-node": "^3.0.2",
    "vitest": "^0.30.1"
  }
}
